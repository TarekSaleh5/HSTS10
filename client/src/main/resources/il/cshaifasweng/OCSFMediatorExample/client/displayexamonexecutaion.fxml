<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="il.cshaifasweng.OCSFMediatorExample.client.DisplayExamOnExecuteController">
	<children>
		<ImageView fitHeight="400.0" fitWidth="210.0"
			layoutX="390.0">
			<image>
				<Image url="@examresults.jpg" />
			</image>
		</ImageView>
		<Button fx:id="signoutbtn" layoutX="522.0" layoutY="14.0"
			mnemonicParsing="false" onAction="#signoutac" text="Log Out">
			<font>
				<Font name="Aharoni Bold" size="12.0" />
			</font>
		</Button>
		<Button fx:id="backbtn" layoutX="522.0" layoutY="363.0"
			mnemonicParsing="false" onAction="#backac" prefHeight="22.0"
			prefWidth="65.0" text="back">
			<font>
				<Font name="Aharoni Bold" size="12.0" />
			</font>
		</Button>
		<ListView fx:id="listViewid" layoutY="-4.0"
			orientation="HORIZONTAL" prefHeight="400.0" prefWidth="390.0" />
		<Button fx:id="addtimebtn" layoutX="398.0" layoutY="71.0"
			mnemonicParsing="false" onAction="#addtimeac" prefHeight="22.0"
			prefWidth="196.0" text="ADD TIME FOR SELECTED EXAM">
			<font>
				<Font name="Aharoni Bold" size="12.0" />
			</font>
		</Button>
		<TextField fx:id="timevaluetxt" layoutX="398.0"
			layoutY="141.0" prefHeight="26.0" prefWidth="77.0" />
		<Button fx:id="submitbtn" layoutX="404.0" layoutY="363.0"
			mnemonicParsing="false" onAction="#submitac" prefHeight="22.0"
			prefWidth="65.0" text="Submit">
			<font>
				<Font name="Aharoni Bold" size="12.0" />
			</font>
		</Button>
		<Label fx:id="noexamisselctedlabel" layoutX="406.0"
			layoutY="93.0" text="No exam is selected!" textFill="RED">
			<font>
				<Font name="System Bold" size="13.0" />
			</font>
		</Label>
		<Label fx:id="invalidtimelabel" layoutX="404.0" layoutY="167.0"
			text="Invalid time!" textFill="RED">
			<font>
				<Font name="System Bold" size="13.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
